{
  "name": "theageofgenz - The Age Of GenZ",
  "short_name": "theageofgenz",
  "description": "News You Can Vibe With - Breaking news, politics, culture & tech from a Gen Z perspective",
  "start_url": "/",
  "id": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#f9fafb",
  "theme_color": "#f97316",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "dir": "ltr",
  "categories": ["news", "entertainment", "lifestyle", "politics", "social"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+theageofgenz",
      "url": "/?share=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "shortcuts": [
    {
      "name": "Breaking News",
      "short_name": "Breaking",
      "description": "Latest breaking news stories",
      "url": "/category/world",
      "icons": [
        {
          "src": "/logo192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Politics",
      "short_name": "Politics",
      "description": "Political news and analysis",
      "url": "/category/politics",
      "icons": [
        {
          "src": "/logo192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
